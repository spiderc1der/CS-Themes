{
  "manifest_version": 3,
  "name": "CS Pink",
  "version": "1.0",
  "description": "Changes the theme of www.chickensmoothie.com to pink!",
  "content_scripts":[
    {"css": ["style.css"],
    "matches": ["https://www.chickensmoothie.com/*"]},
    {"matches": ["https://www.chickensmoothie.com/store/"], 
    "js": ["store.js"]}, 
    {"matches" : ["https://www.chickensmoothie.com/adoptables/"],
    "js": ["adopt.js"]}, 
    {"matches" : ["https://www.chickensmoothie.com/Forum/ucp.php*"], 
    "js" : ["myaccount.js"]},
    {"matches" : ["https://www.chickensmoothie.com/accounts/viewgroup.php?userid=1"], 
    "js" : ["nopets.js"]},
    {"matches" : ["https://www.chickensmoothie.com/accounts/viewgroup.php?*userid=*"],
    "js" : ["viewpets.js"]},
    {"matches" : ["https://www.chickensmoothie.com/viewpet.php?id=*"], 
    "js" : ["viewpet.js"]},
    {"matches" : ["https://www.chickensmoothie.com/stamps/", "https://www.chickensmoothie.com/items/collection/*"], 
    "js" : ["collectionpage.js"]},
    {"matches" : ["https://www.chickensmoothie.com/archive/*/*/Items/"],
    "js" : ["itemsarchive.js"]},
    {"matches" : ["https://www.chickensmoothie.com/archive/*/*/"], 
    "js" : ["petsarchive.js"]},
    {"matches" : ["https://www.chickensmoothie.com/archive/wishlist/*/Items"],
    "js" : ["itemsarchive.js"]},
    {"matches" : ["https://www.chickensmoothie.com/archive/wishlist/*"], 
    "js" : ["petsarchive.js"]},
    {"matches" : ["https://www.chickensmoothie.com/poundandlostandfound.php"],
    "js" : ["pound-and-lost-in-found.js"]},
    {"matches" : ["https://www.chickensmoothie.com/Forum/index.php"],
    "js" : ["forum.js"]},
    {"matches" : ["https://www.chickensmoothie.com/trades/viewtrade.php?id=*"], 
    "js" : ["trade.js"]},
    {"matches" : [], 
    "js" : ["edittrade.js"]},
    {"matches" : ["https://www.chickensmoothie.com/store/*"], 
    "js" : ["storemodels.js"]},
    {"matches" : ["https://www.chickensmoothie.com/help/"], 
    "js" : ["help.js"]},
    {"matches" : ["https://www.chickensmoothie.com/Forum/search.php"], 
    "js" : ["search.js"]},
    {"matches" : ["https://www.chickensmoothie.com/Forum/memberlist.php?mode=viewprofile"],
    "js" : ["viewprofile.js"]},
    {"matches" : ["https://www.chickensmoothie.com/adoptables/*"], 
    "js" : ["storemodels.js"]}
    
  ],
  "web_accessible_resources":[
  ],
  "permissions": [],
  "action": {
    "default_icon": "icon.png",
    "default_title": "CS Pink"
  }
}